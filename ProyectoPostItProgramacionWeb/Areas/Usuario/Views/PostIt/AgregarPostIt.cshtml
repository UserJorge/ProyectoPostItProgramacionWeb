
@{
    ViewData["Title"] = "AgregarPostIt";
}

<h1>AgregarPostIt</h1>

<div class="FormularioGeneral">
    <p class="TituloFormulario">Agregar una nota</p>
    <form method="POST" action="~/Agregar/">
        <p>Nombre del título</p>
        <input type="text" name="Titulo" />
        <p>Descripción</p>
        <input type="text" name="Descripcion" /><br />
        <input type="submit" />
    </form>
</div>